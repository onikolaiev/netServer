    </Simple>
    <Simple>
      <Name>HasResponseTimeout</Name>
      <Type>Byte</Type>
    </Simple>
    <Switch CompareWith="HasResponseTimeout">
      <Case Equals="1">
        <Simple>
          <Name>Response Timeout</Name>
          <Type>PackedDate</Type>
